{
  "name": "grommet-controls",
  "version": "2.0.01",
  "main": "index.js",
  "module": "index.es.js",
  "description": "grommet 2 controls",
  "sideEffects": false,
  "authors": [
    "Atanas Stoyanov"
  ],
  "keywords": [
    "grommet",
    "react",
    "grommet-next",
    "react ui"
  ],
  "homepage": "https://grommet-nextjs.herokuapp.com/add-ons",
  "bugs": "https://github.com/atanasster/grommet-controls/issues",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/atanasster/grommet-controls.git"
  },
  "engine-strict": true,
  "engines": {
    "node": ">= 6.11.5"
  },
  "scripts": {
    "build": "rollup -c && tsc",
    "build-watch": "rollup -c -w",
    "dev": "concurrently --kill-others \"npm run build-watch\" \"npm run start-example\"",
    "storybook": "start-storybook -p 6006 -c .storybook",
    "build-storybook": "build-storybook -c .storybook -o docs",
    "pub": "pushd ./ && npm run build && cd dist && npm publish && popd",
    "debug": "NODE_OPTIONS=--inspect-brk start-storybook -p 6006 -c .storybook"
  },
  "peerDependencies": {
    "grommet": ">= 2.7.10",
    "grommet-icons": ">= 4.1.0",
    "react": ">= 16.8.0",
    "react-dom": ">= 16.8.0",
    "styled-components": ">= 4.1.3"
  },
  "dependencies": {
    "chartjs": "^0.3.24",
    "text-mask-addons": "^3.7.2",
    "text-mask-core": "^5.0.1"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^5.3.0-beta.12",
    "@storybook/addon-a11y": "^5.3.0-beta.12",
    "@storybook/addon-docs": "^5.3.0-beta.12",
    "@storybook/addon-google-analytics": "^5.3.0-beta.12",
    "@storybook/addon-knobs": "^5.3.0-beta.12",
    "@storybook/addons": "^5.3.0-beta.12",
    "@storybook/addon-storysource": "^5.3.0-beta.12",
    "@storybook/react": "^5.3.0-beta.12",
    "@storybook/theming": "^5.3.0-beta.12",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "@types/styled-components": "^4.1.18",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "babel-preset-react-app": "^9.0.2",
    "babel-loader": "^8.0.6",
    "chart.js": "^2.7.2",
    "del": "^5.0.0",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "grommet": "^2.7.10",
    "grommet-icons": "^4.3.0",
    "pre-commit": "^1.2.2",
    "react": "^16.9.0",
    "react-chartjs-2": "^2.7.4",
    "react-docgen-typescript": "^1.12.5",
    "react-dom": "^16.9.0",
    "rollup": "^1.19.4",
    "rollup-plugin-commonjs": "^10.0.2",
    "rollup-plugin-copy-glob": "^0.3.1",
    "rollup-plugin-delete": "^1.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-typescript2": "^0.22.1",
    "storybook-addon-deps": "^1.0.5",
    "storybook-addon-grommet": "^1.0.1",
    "styled-components": "^4.3.2",
    "typescript": "^3.5.3",
    "webpack-react-docgen-typescript": "^0.9.3"
  }
}
